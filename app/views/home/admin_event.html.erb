<div class="container">
	<% if admin_signed_in? %>
		<% @hash.each do |event_name, event_customer| %>
			<h1 style="text-align: center;"><%= event_name %></h1>
			<div ><% event_customer.each do |customer| %>
				<table class="table">
				    <tbody>
				        <tr>
				            <th>2</th>
				            <td><%= customer.email %></td>
				            <td><%= customer.name %></td>
				            <td><%= School.find(customer.school_id).school_name %></td>
				            <td><%= customer.tel %></td>
				        </tr>
				    </tbody>
				</table>
			<% end %>
			</div>
		<% end %>
	<% else %>
		<h1 style="text-align: center; font-weight: 900;">404 Page Not Found</h1>
		<div class="padding_full"></div>
	<% end %>
</div>


