<% if admin_signed_in? %>
	<div class="container">
		<% @sub_tags.each do |st| %>
			<div style="padding-bottom: 5px;">
				<%= st.sub_tag_name %>
				<%= st.sub_tag_detail %>
				<%= link_to "編集",  edit_sub_tag_path(st.id), class: "btn btn-success btn-lg",  method: :get %>
				<%= link_to "削除", sub_tag_path(st.id), class: "btn btn-danger btn-lg", method: :delete %>
			</div>
		<% end %>
	</div>
<% else %>
	<%= link_to "ホームに戻る", root_path %>
	<div class="page_not_found"> 404 page not found</div>
<% end %>